pipeline{
   agent any
   environment{
      PATH = "/opt/apache-maven-3.6.3/bin:$PATH"
   }
   stages{
        stage('git checkout'){
            steps{
                git 'https://github.com/javahometech/myweb'
            }
        }
        stage('maven build'){
            steps{
                sh 'mvn clean package"
            }
        }
        
   }
}
